{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*",
      "use": "@vercel/node"
    },
    {
      "src": "message_live/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/api/(.*)",
      "dest": "api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "message_live/$1"
    }
  ]
}
